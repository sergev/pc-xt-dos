**************************************************************************
* L I S T I N G   A N D  S E T T I N G   V A R I A B L E S  I N  D E M O *
**************************************************************************
*
SET X =30*0
SET NUMBER = 10*0
LIST (X,NUMBER)
AT 18
GO
LIST (I,VAL,X(I,1))
GO
LIST (I,VAL,X(I,1))
*
*** STEPPING THROUGH THE DEMO.  NEXT, STEP, GO, RUN STATEMENTS.
*
NEXT
NEXT
NEXT
NEXT
NEXT
LIST (I,VAL,X(I,1))
STEP
STEP
STEP
STEP
STEP
LIST I
GO
RUN
LIST (I,VAL,X(I,1))
AT 18 OFF
* RERUN DEMO *
W
LB
AT 14
GO 14
AT 14 OFF
W
LB
*
******************************************************************
****************** T R A C I N G   D E M O ***********************
******************************************************************
*
AT 21
TRACE
GO 14
TRACE OFF
TRACE 16:18
GO
AT MYSIN.39
TRACE OFF
TRACE 18:20
ENTRY
GO
AT MYSIN.39 OFF
ENTRY OFF
GO
AT MYSIN.36
ENTRY PARM
GO
LIST (P1, DEMO.VAL, P2, DEMO.N)
W
AT * OFF
AT DEMO./10 OFF
TRACE OFF
ENTRY OFF
GO
W
AT 14
GO 14
AT 14 OFF
W
LB
